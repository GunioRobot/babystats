<% @body_mass_percentiles.each do |body_mass_percentile| %>
  <tr>
    <td><%= body_mass_percentile.month %></td>
    <td><%= body_mass_percentile.l %></td>
    <td><%= body_mass_percentile.mean %></td>
    <td><%= body_mass_percentile.sample_sd %></td>
    <td><%= body_mass_percentile.p01 %></td>
    <td><%= body_mass_percentile.p1 %></td>
    <td><%= body_mass_percentile.p3 %></td>
