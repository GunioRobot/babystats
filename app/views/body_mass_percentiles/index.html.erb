    <td><%= body_mass_percentile.p75 %></td>
    <td><%= body_mass_percentile.p85 %></td>
    <td><%= body_mass_percentile.p90 %></td>
    <td><%= body_mass_percentile.p95 %></td>
    <td><%= body_mass_percentile.p97 %></td>
    <td><%= body_mass_percentile.p99 %></td>
    <td><%= body_mass_percentile.p999 %></td>
    <td><%= body_mass_percentile.gender %></td>
    <td><%= body_mass_percentile.measure_unit %></td>
    <td><%= link_to 'Show', body_mass_percentile %></td>
    <td><%= link_to 'Edit', edit_body_mass_percentile_path(body_mass_percentile) %></td>
    <td><%= link_to 'Destroy', body_mass_percentile, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
